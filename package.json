    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "node-sass": "^4.8.3",
    "popper.js": "^1.14.3",
    "protractor": "~5.3.1",
    "ts-mockito": "^2.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": ">=2.4.2 <2.7.0"
