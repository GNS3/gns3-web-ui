    "material-design-icons": "^3.0.1",
    "ngx-electron": "^1.0.4",
    "npm-check-updates": "^2.14.1",
    "raven-js": "^3.24.1",
    "rxjs": "^5.5.9",
    "typeface-roboto": "^0.0.54",
    "yargs": "^11.0.0",
    "zone.js": "^0.8.26"
  },
